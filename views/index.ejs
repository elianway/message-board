<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div>
      <% messages.forEach((entry) => { %>
        <p><%= entry.user %></p>
        <p><%= entry.text %></p>
        <p><%= entry.added %></p>
      <% }) %>
    </div>
  </body>
</html>
